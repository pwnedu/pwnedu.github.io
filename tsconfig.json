{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "isolatedModules": false,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
    }
  }
}